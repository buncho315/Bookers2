<h1>Books#index</h1>

<table>
  <th>Title</th>
  <th>Opinion</th>

  <body>
    <% @books.each do |book| %>
      <p><%= book.title %></p>
      <p><%= book.body %></p>
    <% end %>
  </body>
  
</table>

<%= render 'users/user_info', user: current_user %>
<%= render 'books/new_book', book: @new_book %>
